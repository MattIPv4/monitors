{
  "name": "monitors",
  "version": "1.0.0",
  "private": true,
  "description": "Uptime monitoring (or really any monitoring that can throw an error) through Node.js & GitHub Actions.",
  "main": "src/locate.js",
  "type": "module",
  "scripts": {
    "locate": "node --experimental-specifier-resolution=node src/locate.js",
    "disabled": "node --experimental-specifier-resolution=node src/disabled.js",
    "monitor": "node --experimental-specifier-resolution=node src/monitor.js",
    "all": "npm run locate | jq -r '.[]' | xargs -L 1 npm run monitor --"
  },
  "author": "Matt (IPv4) Cowley",
  "license": "Apache-2.0",
  "dependencies": {
    "cidr-regex": "^4.0.2",
    "consola": "^2.15.3",
    "console": "^0.7.2",
    "csv-parse": "^5.3.3",
    "gunzip-maybe": "^1.4.2",
    "is-port-reachable": "^4.0.0",
    "node-fetch": "^3.3.0",
    "pg": "^8.8.0",
    "puppeteer": "^19.2.2",
    "ssh2": "^1.11.0"
  }
}
