    "csv-parse": "^5.5.5",
    "gunzip-maybe": "^1.4.2",
    "is-port-reachable": "^4.0.0",
    "pg": "^8.11.3",
    "puppeteer": "^22.5.0",
    "ssh2": "^1.15.0"
  }
}
